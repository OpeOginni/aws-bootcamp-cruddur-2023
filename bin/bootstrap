#! /usr/bin/bash
set -e # stop completely if it fails at any point

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="bootstrap"
printf "${CYAN}===== ${LABEL}${NO_COLOR}\n"

ABS_PATH=$(readlink -f "$0")
BIN_DIR=$(dirname $ABS_PATH)

source "$BIN_DIR/db/setup"
source "$BIN_DIR/dbb/schema-load"
source "$BIN_DIR/dbb/seed"

